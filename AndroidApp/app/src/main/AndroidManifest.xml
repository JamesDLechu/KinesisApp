<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="aplicacin.primerapp.shiro.kinesispowersapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.PowerActivity" />
        <activity android:name=".activities.Power_ExerciseActivity" />
        <activity android:name=".activities.Dictionary_ItemDefinitionActivity" />
        <activity android:name=".activities.Movies_and_SeriesActivity" />
        <activity android:name=".activities.Power_CategoryActivity"/>
    </application>

</manifest>